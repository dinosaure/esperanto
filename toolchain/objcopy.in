# @@CONFIG_TARGET_TRIPLE@@-objcopy: Cosmopolitan wrapper for 'objcopy'.
# Generated by configure.sh at build time.
#
# Passes through to @@CONFIG_TARGET_OBJCOPY@ for actual work.
# We add the --strip-all/-S option in any cases to be sure to strip the
# Cosmopolitan artifact.
S=
for arg do
	shift
	case "$arg" in
		-S)
			S=set
			;;
		--strip-all)
			S=set
			;;
	esac
	set -- "$@" "$arg"
done
[ -z "${S}" ] && S="-S"
[ -n "${__V}" ] && set -x
exec @@CONFIG_TARGET_OBJCOPY@@ \
    ${S} "$@"
